create table inn(
	id serial primary key,
	inn_number int
);

create table human(
	id serial primary key,
	name varchar(255),
	inn_id int references inn(id)
);

insert into inn(inn_number) values (55634);
insert into human(name, inn_id) values ('Vasya', 1);

select * from inn;
select * from inn where id in (select id from human);